#include "main.h"
#include "traffic_light_processing.h"
#include "input_reading.h"
#include "input_processing.h"
#include "global.h"

void traffic_light_processing() {
	switch(status) {
	case MODE:

		break;
	case ADD:
		HAL_TogglePin(GPIOA, D4_Pin);
		HAL_TogglePin(GPIOA, D1_Pin);
		break;
	case CONFIRM:
		HAL_TogglePin(GPIOA, D4_Pin);
		HAL_TogglePin(GPIOA, D1_Pin);
		break;
	case RESET:
		HAL_TogglePin(GPIOA, D4_Pin);
		HAL_TogglePin(GPIOA, D1_Pin);
		break;
	}
}
