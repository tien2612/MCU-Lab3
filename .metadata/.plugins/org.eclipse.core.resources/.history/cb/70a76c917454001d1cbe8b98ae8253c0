#ifndef INC_OUTPUT_DISPLAY_H_
#define INC_OUTPUT_DISPLAY_H_

int _write(int file, char *ptr, int len)
{
  /* Implement your write code here, this is used by puts and printf for example */
  for(int i = 0; i < len; i++) {
	  ITM_SendChar((*ptr++));
  }
  return len;
}

#endif /* INC_OUTPUT_DISPLAY_H_ */
